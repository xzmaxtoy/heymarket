  "description": "Heymarket API endpoint for message management",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "build": "npm install",
    "test": "echo \"Error: no test specified\" && exit 1",
