  "dependencies": {
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-rate-limit": "^6.0.0",
    "helmet": "^4.6.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.0",
